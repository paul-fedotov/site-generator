{
  "name": "site-generator",
  "version": "0.0.1",
  "description": "Applications for generating sites from a set of components. Uses React, Redux, Next.js, MongoDB, Node.js.",
  "main": "index.js",
  "scripts": {
    "dev": "cd ./src && PORT=3030 BASE_URL=/lp node server.js",
    "build": "next build ./src",
    "start": "cd ./src && NODE_ENV=production PORT=3030 BASE_URL=/lp node server.js"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "express": "^4.17.1",
    "next": "^11.1.0",
    "next-routes": "^1.4.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-jss": "^8.6.1",
    "react-redux": "^7.1.1",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paul-fedotov/site-generator.git"
  },
  "keywords": [
    "React",
    "Redux",
    "Next.js",
    "MongoDB"
  ],
  "author": "Paul Fedotov",
